#include <stdio.h>

void findDuplicates(int arr[], int size) {
    int found = 0;

    printf("Duplicate elements in the array:\n");
    for (int i = 0; i < size; i++) {
        // Check if this element has been checked before
        int isDuplicate = 0;
        for (int k = 0; k < i; k++) {
            if (arr[i] == arr[k]) {
                isDuplicate = 1;
                break;
            }
        }
        if (isDuplicate) continue;

        // Count occurrences
        int count = 0;
        for (int j = i + 1; j < size; j++) {
            if (arr[i] == arr[j]) {
                count++;
            }
        }

        if (count > 0) {
            printf("%d\n", arr[i]);
            found = 1;
        }
    }

    if (!found) {
        printf("No duplicates found.\n");
    }
}

int main() {
    int arr[] = {4, 2, 7, 2, 8, 7, 1, 4};
    int size = sizeof(arr) / sizeof(arr[0]);

    findDuplicates(arr, size);

    return 0;
}
